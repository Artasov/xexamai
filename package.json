{
  "name": "xexamai",
  "version": "0.8.2",
  "description": "AI Interview Assistant",
  "main": "dist/main/app.js",
  "type": "commonjs",
  "author": "xexamai",
  "license": "GPL-3.0",
  "scripts": {
    "prestart": "npm run build:assets",
    "start": "electron .",
    "dev": "cross-env NODE_ENV=development concurrently \"npm:dev:main\" \"npm:dev:renderer\" \"npm:dev:electron\"",
    "dev:main": "tsup --entry.app src/main/app.ts --entry.preload src/preload/index.ts --format=cjs --out-dir dist/main --watch --external electron --target=node20 --sourcemap",
    "dev:renderer": "vite --config vite.config.mts",
    "dev:electron": "cross-env VITE_DEV_SERVER_URL=http://localhost:5174 wait-on http://localhost:5174 file:dist/main/app.js file:dist/main/preload.js && electron .",
    "build": "npm run build:assets && electron-builder --publish=never",
    "build:win": "npm run build:assets && electron-builder --win --publish=never",
    "build:mac": "npm run build:assets && electron-builder --mac --publish=never",
    "build:linux": "npm run build:assets && electron-builder --linux --publish=never",
    "build:all": "npm run build:assets && electron-builder -mwl --publish=never",
    "build:ci": "npm run build:assets && electron-builder --publish=always",
    "build:assets": "cross-env NODE_ENV=production npm run build:main && npm run build:renderer",
    "build:main": "tsup --entry.app src/main/app.ts --entry.preload src/preload/index.ts --format=cjs --sourcemap --out-dir dist/main --external electron --target=node20",
    "build:renderer": "vite build --config vite.config.mts",
    "clean": "node scripts/clean.js",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.16",
    "@types/node": "24.9.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "5.1.0",
    "autoprefixer": "10.4.21",
    "concurrently": "9.2.1",
    "cross-env": "10.1.0",
    "electron": "38.4.0",
    "electron-builder": "26.0.12",
    "postcss": "8.5.6",
    "sass": "1.93.2",
    "tailwindcss": "4.1.16",
    "tslib": "2.8.1",
    "tsup": "8.5.0",
    "typescript": "5.9.3",
    "vite": "7.1.12",
    "wait-on": "9.0.1"
  },
  "dependencies": {
    "@google/genai": "1.22.0",
    "@solana/web3.js": "1.98.4",
    "dotenv": "17.2.3",
    "electron-audio-loopback": "1.0.6",
    "electron-store": "11.0.2",
    "marked": "16.3.0",
    "openai": "6.1.0",
    "qrcode.react": "4.2.0",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  }
}
